{
  "env": {
	"browser": true,
	"es6": true,
	  "node": true
  },
  "extends": [
	  "eslint:recommended",
	  "plugin:react/recommended",
	  "plugin:jest/recommended"
  ],
  "globals": {
	"Atomics": "readonly",
	"SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
	"ecmaFeatures": {
	  "jsx": true
	},
	"ecmaVersion": 2021,
	"sourceType": "module"
  },
  "plugins": [
	"jest"
  ],
  "rules": {
	"prefer-template": "off",
	  "react/prop-types": "off"
  }
}

{
  "env": {
	"browser": true,
	"es6": true
  },
  "extends": [
	"preact",
	"plugin:jest/recommended"
  ],
  "globals": {
	"Atomics": "readonly",
	"SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
	"ecmaFeatures": {
	  "jsx": true
	},
	"ecmaVersion": 2021,
	"sourceType": "module"
  },
  "plugins": [
	"jest"
  ],
  "rules": {
	"prefer-template": "off"
  }
}
